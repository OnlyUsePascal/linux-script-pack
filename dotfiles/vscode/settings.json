{
  "files.autoSaveDelay": 10,

  // layout
  "workbench.tree.indent": 19,
  "explorer.sortOrder": "type",
  "window.restoreWindows": "none",
  "terminal.integrated.commandsToSkipShell": [
    // "workbench.action.toggleSidebarVisibility",
    // "workbench.action.navigateUp",
    // "workbench.action.focusActiveEditorGroup",
  ],

  // behavior
  "C_Cpp.intelliSenseUpdateDelay": 500,
  "C_Cpp.codeAnalysis.updateDelay": 500,

  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,

  "editor.inlineSuggest.enabled": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.quickSuggestionsDelay": 5,
  "editor.autoClosingOvertype": "always",
  "editor.trimAutoWhitespace": false,

  "window.openFoldersInNewWindow": "off",
  "window.enableMenuBarMnemonics": false,
  "window.commandCenter": false,

  "security.workspace.trust.untrustedFiles": "open",
  "git.openRepositoryInParentFolders": "never",

  "workbench.panel.defaultLocation": "bottom",
  "workbench.layoutControl.type": "toggles",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "files.associations": {
    "*.desktop": "ini"
  },

  // theme
  // "terminal.integrated.cwd": "${fileDirname}",
  // "terminal.integrated.defaultLocation": ,: ,
  "debug.console.fontSize": 13,
  "terminal.integrated.fontSize": 13,
  "editor.fontSize": 13,
  "editor.lineHeight": 1.5,
  "window.zoomLevel": 0.6,
  "editor.fontFamily": "Jetbrains Mono",
  "apc.font.family": "Jetbrains Mono",
  "editor.cursorWidth": 3,
  "workbench.iconTheme": "material-icon-theme",
  "editor.wordWrapColumn": 180,
  "todohighlight.isEnable": true,
  "editor.fontLigatures": true,
  "alt-catppuccin.customUIColors": {},
  "window.menuBarVisibility": "hidden",

  "explorer.confirmPasteNative": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "extensions.autoCheckUpdates": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "code-runner.runInTerminal": true,
  "diffEditor.wordWrap": "off",

  // python
  "python.analysis.indexing": true,
  "python.analysis.autoImportCompletions": true,
  "python.languageServer": "Pylance",

  // latex
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "env": {}
    }
  ],

  "latex-workshop.latex.recipes": [
    {
      "name": "pdfLaTeX",
      "tools": ["pdflatex"]
    },
    {
      "name": "latexmk ðŸ”ƒ",
      "tools": ["latexmk"]
    },
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "pdflatex âžž bibtex âžž pdflatex`Ã—2",
      "tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
    },
    {
      "name": "xelatex âžž bibtex âžž xelatex`Ã—2",
      "tools": ["xelatex", "bibtex", "xelatex", "xelatex"]
    }
  ],

  // js
  "eslint.debug": true,

  "terminal.integrated.defaultProfile.linux": "zsh",
  "window.title": "${rootName}${separator}${remoteName}${separator}VSCode",

  "cmake.showOptionsMovedNotification": false,
  "editor.linkedEditing": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "thunder-client.defaultURL": "http://localhost:8080/",
  "mdb.confirmRunAll": false,

  // path intellisense
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

